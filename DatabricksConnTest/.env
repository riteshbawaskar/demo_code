# .env file template for Databricks SPN verification

# Databricks Workspace Configuration
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com
DATABRICKS_TOKEN=dapi1234567890abcdef  # Your personal access token

# Service Principal Details
SERVICE_PRINCIPAL_APP_ID=12345678-1234-1234-1234-123456789abc  # Application ID from Azure AD
SERVICE_PRINCIPAL_CLIENT_ID=12345678-1234-1234-1234-123456789abc  # Same as App ID usually
SERVICE_PRINCIPAL_SECRET=your-client-secret-value

# Azure AD Configuration (for Azure Databricks)
AZURE_TENANT_ID=87654321-4321-4321-4321-210987654321

# Optional: Specific Resources to Check
SQL_WAREHOUSE_ID=abc123def456  # Optional - from SQL Warehouse connection details
CLUSTER_ID=0123-456789-abcdef  # Optional - from Cluster details